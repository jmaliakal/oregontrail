<%= form_for(@leader) do |f| %>

    <% professions = ['Farmer', 'Banker', 'Carpenter'] %>

    <%= f.label :name %>
    <%= f.text_field(:name) %>

    <%= f.label :profession %>
    <%= f.select(:profession, professions) %>

    <%= f.hidden_field(:health, :value => 10) %>
    <%= f.hidden_field(:traveling_party) %>

<% end %>
